            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@ImagenPrincipal.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
      <VBox alignment="BASELINE_CENTER" prefHeight="162.0" prefWidth="775.0" VBox.vgrow="SOMETIMES">
